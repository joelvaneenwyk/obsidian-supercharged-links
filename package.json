    "eslint-plugin-unicorn": "^48.0.1",
    "ist": "^1.1.7",
    "jest": "^29.7.0",
    "obsidian": "^1.4.11",
    "obsidian-dataview": "^0.5.61",
    "prettier": "^3.0.3",
    "shx": "^0.3.4",
    "style-mod": "^4.1.0",
    "svelte": "^4.2.2",
    "ts-node": "^10.9.1",
