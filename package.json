{
  "name": "mycoshiro-supercharged-links",
  "version": "0.7.3",
  "description": "Add properties and menu options to links and style them!",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "check": "tsc --skipLibCheck --noEmit",
    "build": "node esbuild.config.mjs production",
    "deploy": "cross-env-shell \"yarn shx --verbose mkdir -p \"${PROJECT_CWD}/.obsidian/plugins/${npm_package_name}\" && yarn shx --verbose cp -u styles.css dist/main.js manifest.json versions.json \"${PROJECT_CWD}/.obsidian/plugins/${npm_package_name}\" \"",
    "lint": "yarn check && eslint \"**/*.ts\" \"**/*.mjs\"",
    "lint:fix": "eslint \"**/*.ts\" \"**/*.mjs\" --fix",
    "release": "yarn build && yarn deploy"
  },
  "keywords": [],
  "author": "Joel Van Eenwyk",
  "authorUrl": "https://github.com/joelvaneenwyk/",
  "repository": {
    "type": "git",
    "url": "https://github.com/joelvaneenwyk/mycoshiro-supercharged-links.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.22.11",
    "@codemirror/buildhelper": "^1.0.0",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "@lezer/common": "^1.0.4",
    "@lezer/highlight": "^1.1.6",
    "@lezer/javascript": "^1.4.7",
    "@lezer/lr": "^1.0.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.31",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@yarnpkg/sdks": "^3.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-import-glob": "^2.0.0",
    "builtin-modules": "3.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.5",
    "esbuild-plugin-import-glob": "^0.1.1",
    "esbuild-plugin-replace": "^1.4.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-json-files": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
