    "@lezer/common": "^1.0.0",
    "@lezer/highlight": "^1.0.0",
    "@lezer/javascript": "^1.0.0",
    "@lezer/lr": "^1.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@yarnpkg/sdks": "^3.0.0-rc.49",
    "builtin-modules": "3.3.0",
    "compare-versions": "^4.1.1",
    "cross-env": "^7.0.3",
